[Unit]
Description=xivo-amid server
ConditionPathExists=!/tmp/xivo-disabled
After=network.target asterisk.service postgresql.service xivo-confgend.service

[Service]
Type=forking
ExecStartPre=/usr/bin/install -d -o www-data -g www-data /var/run/xivo-amid
ExecStart=/usr/bin/xivo-amid
PIDFile=/var/run/xivo-amid/xivo-amid.pid

[Install]
WantedBy=multi-user.target
